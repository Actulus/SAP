{
	"_version": "1.5.0",
	"sap.app": {
		"id": "app.app",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"dataSources": {
			"TLURemote": {
				"uri": "/Sales.xsodata",
				"type": "OData",
				"settings": {
					"ODataVersion": "2.0"
				}
			}
		},
		"sourceTemplate": {
			"id": "hanatemplates.basicSAPUI5ApplicationProject",
			"version": "0.0.0"
		}
	},

	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_bluecrystal"
		]
	},

	"sap.ui5": {
		"rootView": {
			"viewName": "app.app.view.View1",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.f": {},
				"sap.ushell": {},
				"sap.collaboration": {},
				"sap.ui.comp": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "app.app.i18n.i18n"
				}
			},
			"": {
				"_comment": "Standard Model",
				"dataSource": "TLURemote",
				"type": "sap.ui.model.odata.v2.ODataModel",
				"preload": true,
				"settings": {
					"useBatch": false,
					"json": true,
					"defaultBindingMode": "TwoWay",
					"defaultUpdateMethod": "PUT"
				}
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"transition": "slide",
				"viewPath": "app.app.view",
				"controlAggregation": "pages",
				"controlId": "idAppControl"
			},
			"routes": [{
				"name": "RouteView1",
				"pattern": "",
				"target": ["TargetView1"]
			}, {
				"name": "RouteView2",
				"pattern": "details",
				"target": "TargetView2"
			}],
			"targets": {
				"TargetView1": {
					"viewName": "home",
					"viewlevel": 1
				},
				"TargetView2": {
					"viewName": "details",
					"transition": "show"
				}
			}
		}
	}
}